if 10>1:
    print("10 greater than 1")

if 2**3>5:
    print("True")

if 3+3*3>10:
    print("10 is lower")

if 4**2>15:
    print("4**2 is greater than 15")

if 99*99>1000:
    print("99*99 is greater than 1000")

print("Hello, Python")
print(5)
print(10 + 20)
print("I like programming")
print(100 * 2)
