i = 1
while i <= 5:
    if i == 3:
        break
    print(i)
    i += 1

j = 0
while j < 5:
    j += 1
    if j % 2 == 0:
        continue
    print(j)

k = 1
while k <= 3:
    print(k)
    k += 1
else:
    print("Finished loop")

n = 1
while n <= 5:
    if n == 4:
        break
    if n % 2 == 0:
        n += 1
        continue
    print(n)
    n += 1
else:
    print("Loop completed")

m = 1
while m <= 3:
    print(m)
    m += 1
else:
    print("All iterations done")

