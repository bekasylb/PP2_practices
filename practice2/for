for i in range(1, 6):
    if i == 3:
        break
    print(i)

for j in range(1, 6):
    if j % 2 == 0:
        continue
    print(j)

for k in range(1, 4):
    print(k)
else:
    print("Finished loop")

for n in range(1, 6):
    if n == 4:
        break
    if n % 2 == 0:
        continue
    print(n)
else:
    print("Loop completed")

for m in range(1, 4):
    print(m)
else:
    print("All iterations done")

